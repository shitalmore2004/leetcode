
class Solution {
    public boolean isAcronym(List<String> words, String s) {
        StringBuilder acr=new StringBuilder();
        for(String word:words){
            char  ch = word.charAt(0);
            acr.append(ch);
        }
        if(acr.toString().equals(s)){
            return true;
        }
        return false;
        
    }
}
