class Solution {
    public String longestCommonPrefix(String[] strs) {
        String str1=strs[0];
        String str2=strs[strs.length-1];
        Arrays.sort(strs);
        int index =0;
        while(index<strs.length){
            if(str1.charAt(index)==str2.charAt(index)){
                index++;
            }else{
                break;
            }

        }
        return index==0?"":str1.substring(0,index);
    }
}
